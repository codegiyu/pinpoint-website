    "nuqs": "^2.4.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-intersection-observer": "^9.16.0",
    "sonner": "^2.0.6",
    "swiper": "^11.2.10",
    "tailwind-merge": "^3.3.1",
    "vaul": "^1.1.2",
    "zod": "^4.0.5",
    "zustand": "^5.0.6"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@svgr/cli": "^8.1.0",
    "@tailwindcss/postcss": "^4",
    "@types/formidable": "^3.4.5",
